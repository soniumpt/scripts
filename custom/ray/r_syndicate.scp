[ITEMDEF i_resurrect_stone_syndicate]
ID=0ed6
NAME=Syndicate Resurrection Stone

ON=@CREATE
   ATTR=010
   COLOR=07d1

ON=@DClick
   IF (<SRC.ISSYNDICATE>)
      IF (<SRC.ISDEAD>)
          SRC.RESURRECT
          SRC.SYSMESSAGE @07d1 The ways of syndicate flow through you once more...
	  ELSE
		  SRC.SYSMESSAGE @07d1 Check again, you are not dead!
      ENDIF
   ELSE
      SRC.SYSMESSAGE @07d1 Only members of Syndicate can use this stone.
   ENDIF
   RETURN 1

