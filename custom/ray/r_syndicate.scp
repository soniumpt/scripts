ENDIF

ON=*hi* 
ON=*hello*
ON=*hail*
ON=*Greetings*
SAY @07d1 If you want to try to join our Syndicate, say "join".
SAY @07d1 If you want to know more information about your character, say "rank".

ON=*join*
IF (<SRC.ISMILITANT>)
SAY @07d1 I am sorry, trader. I cannot deal with your business while you are part of a Militia.
RETURN 1
ENDIF

IF (<SRC.ISSYNDICATE>)
SAY @07d1 You already belong to the Syndicate.
RETURN 1

ELSEIF (<SRC.ISACADEMY>)
SAY @07d1 You belong to the Academy. You can't join us yet!
RETURN 1

ELSEIF (<SRC.ISORDER>)
SAY @07d1 You belong to the Order Faction. You can't join us.
RETURN 1

ELSEIF (<SRC.ISCHAOS>)
SAY @07d1 You belong to the Chaos Faction. You can't join us.
RETURN 1

ELSEIF (<SRC.ISBROTHERHOOD>)
SAY @07d1 You belong to the Brotherhood. You can't join us.
RETURN 1

ELSEIF (<SRC.ISMERCENARY>)
SAY @07d1 You belong to the Mercenaries. You can't join us.

ELSEIF !(<SRC.ISSYNDICATE>) && (<SRC.MINING> > 800) && (<SRC.BLACKSMITHING> > 800)
DIALOG d_syndicate_join
ELSEIF !(<SRC.ISSYNDICATE>) && (<SRC.LUMBERJACKING> > 800) && (<SRC.CARPENTRY> > 800)
DIALOG d_syndicate_join
ELSE
SAY @07d1 You don't have enough skill to join our syndicate yet.
ENDIF
RETURN 1

