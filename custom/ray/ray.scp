	msh_debug NW Corner: (<eval <SRC.CTAG0.MSHNW.X>>,<eval <SRC.CTAG0.MSHNW.Y>>)
	msh_debug SE Corner: (<eval <SRC.CTAG0.MSHSE.X>>,<eval <SRC.CTAG0.MSHSE.Y>>)
