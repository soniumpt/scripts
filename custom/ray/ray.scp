      SRC.DAMAGE <dLOCAL.DamageAmount> <LOCAL.DamageFlags> <LINK>
   ENDIF
ON=@Timer
   IF (<MORE1>)
      DORAND 11
         SOUND=284
         SOUND=285
         SOUND=286
         SOUND=287
         SOUND=774
         SOUND=775
         SOUND=776
         SOUND=777
      ENDDO
      TIMER=2
      MORE1=0
      LOCAL.DamageSource=<LINK>
      FORCHARS 2
         f_meteor_damage <LOCAL.DamageSource> <TAG0.MeteorType>
      ENDFOR
      RETURN 1
   ENDIF
   REMOVE
   RETURN 1

